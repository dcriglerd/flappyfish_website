{
  "expo": {
    "name": "Flappy Fish",
    "slug": "flappy-fish",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00b4d8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flappyfish.game",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          { "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork" },
          { "SKAdNetworkIdentifier": "4fzdc2evr5.skadnetwork" },
          { "SKAdNetworkIdentifier": "4pfyvq9l8r.skadnetwork" },
          { "SKAdNetworkIdentifier": "2fnua5tdw4.skadnetwork" },
          { "SKAdNetworkIdentifier": "ydx93a7ass.skadnetwork" },
          { "SKAdNetworkIdentifier": "5a6flpkh64.skadnetwork" },
          { "SKAdNetworkIdentifier": "p78axxw29g.skadnetwork" },
          { "SKAdNetworkIdentifier": "v72qych5uu.skadnetwork" },
          { "SKAdNetworkIdentifier": "ludvb6z3bs.skadnetwork" },
          { "SKAdNetworkIdentifier": "cp8zw746q7.skadnetwork" }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00b4d8"
      },
      "package": "com.flappyfish.game",
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9210526164379066~4938293330",
          "iosAppId": "ca-app-pub-9210526164379066~4938293330"
        }
      ],
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-9210526164379066~4938293330",
    "ios_app_id": "ca-app-pub-9210526164379066~4938293330"
  }
}
